<%= erb :header_partial %>

<div class="row">
  <div class="col-lg-12">
    <h1>Vulnreport <small><span style="color:#B40404;"><i class="fa fa-exclamation"></i> Warning</span></small></h1>
    <ol class="breadcrumb">
      <li><a href="/"><i class="fa fa-dashboard"></i> Dashboard</a></li>
      <li class="active"><i class="fa fa-exclamation"></i> Warning</li>
    </ol>
  </div>
</div><!-- /.row -->

<div class="row">
  <div class="col-lg-1" style="text-align:center;"></div>
  <div class="col-lg-10" style="text-align:center;">
    <h4>This Application is marked as private and you are not listed as an authorized viewer</h4>

    <p style="margin-bottom:22px;">Because of your user access level, you will be allowed to view/modify the test if you affirm that you are authorized to view this data. 
    If you do not have explicit authorization to view this test you should not proceed. This test may contain sensitive or confidential data.</p>

    <a href="javascript: history.go(-1)"><button class="btn btn-danger">Cancel</button></a>
    <% if !@aid.nil? %>
      <a href="/reviews/<%=@aid%>?warned=true"><button class="btn btn-success">Proceed</button></a>
    <% else %>
      <a href="/tests/<%=@tid%>?warned=true"><button class="btn btn-success">Proceed</button></a>
    <% end %>
  </div>
  <div class="col-lg-1" style="text-align:center;"></div>
</div>


<%= erb :footer_partial %>